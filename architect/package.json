{
    "name": "architect",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "add": "^2.0.6",
        "autoprefixer": "7.1.6",
        "axios": "^0.18.0",
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-preset-react-app": "^3.1.2",
        "babel-runtime": "6.26.0",
        "case-sensitive-paths-webpack-plugin": "2.1.1",
        "chalk": "1.1.3",
        "css-loader": "0.28.7",
        "dotenv": "4.0.0",
        "dotenv-expand": "4.2.0",
        "eslint": "^5.6.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-react-app": "^2.1.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-flowtype": "2.39.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.1",
        "eslint-plugin-react": "^7.11.1",
        "extract-text-webpack-plugin": "3.0.2",
        "farce": "^0.2.6",
        "file-loader": "1.1.5",
        "found": "^0.3.14",
        "found-relay": "^0.3.1",
        "fs-extra": "3.0.1",
        "graphql": "^14.0.2",
        "html-webpack-plugin": "2.29.0",
        "immutable": "^3.8.2",
        "jest": "20.0.4",
        "node-sass-chokidar": "^1.3.3",
        "npm-run-all": "^4.1.3",
        "object-assign": "4.1.1",
        "postcss-flexbugs-fixes": "3.2.0",
        "postcss-loader": "2.0.8",
        "prettier": "^1.14.3",
        "promise": "8.0.1",
        "prop-types": "^15.6.2",
        "raf": "3.4.0",
        "react": "^16.5.2",
        "react-dev-utils": "^5.0.2",
        "react-dom": "^16.5.2",
        "react-helmet": "^5.2.0",
        "react-redux": "^5.0.7",
        "react-relay": "^1.6.2",
        "redux": "^4.0.0",
        "redux-immutable": "^4.0.0",
        "redux-saga": "^0.16.0",
        "relay-runtime": "^1.6.2",
        "resolve": "1.6.0",
        "style-loader": "0.19.0",
        "sw-precache-webpack-plugin": "0.11.4",
        "url-loader": "0.6.2",
        "webpack": "3.8.1",
        "webpack-dev-server": "2.11.3",
        "webpack-manifest-plugin": "1.3.2",
        "whatwg-fetch": "2.0.3",
        "yarn": "^1.9.4"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/styles -o src/styles",
        "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles --watch --recursive",
        "start-js": "node scripts/start.js",
        "build-js": "node scripts/build.js",
        "dev": "REACT_APP_ENV=dev npm-run-all -p watch-css start-js",
        "build-uat": "REACT_APP_ENV=uat npm-run-all build-css build-js",
        "build-liv": "REACT_APP_ENV=production npm-run-all build-css build-js",
        "test": "node scripts/test.js --env=jsdom",
        "prettier": "find . \\( -name node_modules -o -name __generated__ \\) -prune -or \\( -name '*.js' -o -name '*.css' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node",
            "mjs"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ],
        "plugins": [
            "relay"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "devDependencies": {
        "babel-plugin-relay": "^1.6.2",
        "relay-compiler": "^1.6.2"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "bracketSpacing": true,
        "jsxBracketSameLine": true,
        "tabWidth": 4,
        "printWidth": 140
    }
}
